
<h1>Sign Up</h1>
<div class="row">
	<div class="col-md-5">
<%=form_tag("/login/create",:method=>"get") do %>
 <div class="form-group">
<%=label_tag :Username %>
<%=text_field_tag :username ,nil,{:class=>"form-control"} %>
</div>
<div class="from-group">
 <%=label_tag :Password %>
 <%=password_field_tag :password ,nil,{:class=>"form-control"}%> <br>
</div>
<div class="form-group">
<%=button_tag :SignUp ,:class=>"btn btn-success"%>
</div>
<% end %>
</div>
</div>
