<h1>welcome</h1>

<div class="row">
<div class="col-md-4">
<%=form_tag("/login/check",:method=>"get") do%>

<div class="form-group">
<%=label_tag :Username %>
<%=text_field_tag :username,nil,{:class=>"form-control"} %><br>
</div>

<%=label_tag :Password %>
<%=password_field_tag :password,nil,{:class=>"form-control"}%><br>

<%=button_tag :Login,:class=>"btn btn-success"%>




<%end%>
</div>
</div>